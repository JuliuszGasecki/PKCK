<Window x:Class="GUI.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        xmlns:viewModel="clr-namespace:GUI.ViewModel"
        mc:Ignorable="d"
        Title="MainWindow" Height="1000" Width="800"
        ResizeMode="NoResize">
    <Window.DataContext>
        <viewModel:MainViewModel/>
    </Window.DataContext>
    <Grid>
        <ScrollViewer>
            <TextBlock Text="{Binding XmlString}" Name="XmlTextBlock" HorizontalAlignment="Left" Margin="315,22,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="auto" Width="452"/>
        </ScrollViewer>
    </Grid>
</Window>
